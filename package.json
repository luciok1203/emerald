{
  "name": "wafflestudio-2025-fe-seminar-template",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "biome lint .",
    "check:types": "tsc",
    "check:format": "biome check .",
    "check:unused": "knip",
    "check-all": "yarn check:types && yarn check:format && yarn check:unused",
    "fix": "biome check --write --unsafe .",
    "fix-all": "yarn fix && yarn check-all",
    "ready": "yarn fix-all && yarn build"
  },
  "dependencies": {
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.12.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "knip": "^5.59.1",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  }
}
